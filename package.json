  "preferGlobal": true,
  "bin": {
    "cnpm": "bin/cnpm"
  },
  "dependencies": {
    "auto-correct": "^1.0.0",
    "bagpipe": "^0.3.5",
    "colors": "^1.4.0",
    "commander": "^2.20.0",
    "cross-spawn": "^6.0.5",
    "debug": "^4.3.1",
    "giturl": "^1.0.1",
    "ini": "^1.3.5",
    "npm": "^7.10.0",
    "npm-request": "^1.0.0",
    "npminstall": "^4.11.0",
    "open": "^8.0.6",
    "urllib": "^2.37.1"
  },
  "devDependencies": {
    "autod": "^2.7.1",
    "coffee": "^5.2.1",
    "egg-bin": "^4.7.0",
    "egg-ci": "^1.1.0",
    "eslint": "^4.19.1",
    "eslint-config-egg": "^7.0.0",
    "mz-modules": "^2.1.0"
  },
  "homepage": "https://github.com/cnpm/cnpm",
  "repository": {
