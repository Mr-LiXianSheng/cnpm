{
  "name": "cnpm",
  "version": "6.2.0",
  "description": "cnpm: npm client for cnpmjs.org",
  "main": "bin/cnpm",
  "files": [
    "bin",
    "lib"
  ],
  "scripts": {
    "test": "npm run lint -- --fix && npm run test-local",
